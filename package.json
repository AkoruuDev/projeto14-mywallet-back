{
    "name": "projeto14-mywallet-back",
    "version": "2.0.0",
    "description": "A back end project about a digital wallet",
    "type": "module",
    "main": "src/server.ts",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "update": "nvm use v20.5.1",
        "dist": "npx tsc",
        "dev": "npx nodemon src/server.ts",
        "start:back": "npm i express cors dotenv jsonwebtoken dayjs bcrypt joi http-status",
        "start:ts": "npm i -D typescript ts-node @types/express @types/cors @types/dotenv",
        "prisma": "npm i prisma",
        "prisma:init": "npx prisma init",
        "prisma:dbpull": "npx prisma db pull",
        "prisma:generate": "npx prisma generate"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/AkoruuDev/projeto14-mywallet-back.git"
    },
    "author": "AkoruuDev",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/AkoruuDev/projeto14-mywallet-back/issues"
    },
    "homepage": "https://github.com/AkoruuDev/projeto14-mywallet-back#readme",
    "dependencies": {
        "bcrypt": "^5.1.0",
        "cors": "^2.8.5",
        "dayjs": "^1.11.9",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "http-status": "^1.6.2",
        "joi": "^17.9.2",
        "jsonwebtoken": "^9.0.1",
        "prisma": "^5.1.1"
    },
    "devDependencies": {
        "@types/cors": "^2.8.13",
        "@types/dotenv": "^8.2.0",
